<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
    app:startDestination="@id/splashFragment">
  <fragment
    android:id="@+id/passFragmentNav"
    android:name="com.android.aftools.presentation.fragments.EnterPassFragment">
    <action
        android:id="@+id/action_passFragmentNav_to_settingsFragment"
        app:destination="@id/settingsGraph"
        app:popUpTo="@id/passFragmentNav"
        app:popUpToInclusive="true" />
  </fragment>
  <fragment
    android:id="@+id/setupFilesFragment"
    android:name="com.android.aftools.presentation.fragments.SetupFilesFragment" />
  <fragment
    android:id="@+id/logsFragment"
    android:name="com.android.aftools.presentation.fragments.LogsFragment" />
  <fragment
      android:id="@+id/aboutFragment"
      android:name="com.android.aftools.presentation.fragments.AboutFragment" />
  <fragment
      android:id="@+id/profilesFragment"
      android:name="com.android.aftools.presentation.fragments.ProfilesFragment" />
  <fragment
      android:id="@+id/rootFragment"
      android:name="com.android.aftools.presentation.fragments.RootFragment" />
  <fragment
      android:id="@+id/splashFragment"
      android:name="com.android.aftools.presentation.fragments.SplashFragment">
    <action
        android:id="@+id/action_splashFragment_to_setupPassFragment"
        app:destination="@id/setupPassFragment"
        app:popUpTo="@id/splashFragment"
        app:popUpToInclusive="true">
      <argument
          android:name="fromSplash"
          android:defaultValue="true" />
    </action>
    <action
        android:id="@+id/action_splashFragment_to_passFragmentNav"
        app:destination="@id/passFragmentNav"
        app:popUpTo="@id/splashFragment"
        app:popUpToInclusive="true" />
  </fragment>
  <fragment
      android:id="@+id/setupPassFragment"
      android:name="com.android.aftools.presentation.fragments.SetupPassFragment">
    <action
        android:id="@+id/action_setupPassFragment_to_settingsFragment"
        app:destination="@id/settingsGraph"
        app:popUpTo="@id/setupPassFragment"
        app:popUpToInclusive="true" />
    <argument
        android:name="fromSplash"
        android:defaultValue="false"
        app:argType="boolean" />
  </fragment>
  <include app:graph="@navigation/settings_graph">

  </include>
</navigation>
